{
  "name": "react-tetris",
  "version": "1.0.0",
  "description": "Tetris Game implementation in reactjs",
  "main": "index.js",
  "homepage": "http://gautamnaik1994.github.io/React-Tetris",
  "scripts": {
    "start": "webpack-dev-server --hot --mode development --config webpack.config.dev.js",
    "prebuild": "rimraf dist",
    "build": "webpack --mode production --config webpack.config.prod.js",
    "test": "jest",
    "pretty": "prettier --write \"**/*.js\" --config .prettierrc",
    "format": "prettier-eslint --write \"src/**/*.js\" --config .prettierrc --eslint-config-path .eslintrc",
    "pretty-scss": "prettier --write \"**/*.scss\" --config .prettierrc",
    "format2": "prettier --write '**/*.{js,scss}' --config .prettierrc",
    "precommit": "lint-staged",
    "deploy": "gh-pages -d dist"
  },
  "author": "Gautam Naik",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/preset-stage-2": "^7.0.0-beta.46",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.0-beta.2",
    "babel-polyfill": "^7.0.0-beta.3",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "error-overlay-webpack-plugin": "^0.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.1",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.5",
    "prettier-eslint-cli": "^4.7.1",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.3.2",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.11.2",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2",
    "webpack-pwa-manifest": "^3.6.2",
    "webpackbar": "^2.6.1",
    "workbox-webpack-plugin": "^3.2.0"
  },
  "dependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0"
  },
  "browserslist": [
    "> 1%",
    "IE 10",
    "last 2 versions"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/test/jest-setup.js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "testMatch": [
      "**/__tests__/*.(js)"
    ]
  },
  "lint-staged": {
    "*.{js,scss}": [
      "npm run pretty",
      "npm run pretty-scss",
      "git add"
    ]
  }
}
